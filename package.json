{
  "name": "biketag-vue",
  "version": "3.2.7",
  "license": "AGPL-3.0-or-later",
  "author": "Ken Eucker",
  "bugs": {
    "url": "https://github.com/keneucker/biketag-vue/issues"
  },
  "scripts": {
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --ext .ts,.js,.vue src --fix",
    "lint:style": "stylelint \"./src/**/*.vue\" \"./src/**/*.scss\" --fix",
    "serve": "vite",
    "pub:expose": "mv dist/index.d.ts ./index.d.ts && mv dist/biketag-vue.* ./ && mv dist/components ./components && mv dist/common ./common && mv dist/store ./store",
    "pub:clear": "rm -f ./index.d.ts ./biketag-vue.es.js ./biketag-vue.css && rm -rf dist ./components ./common ./store",
    "pub:build": "vue-tsc && vite build --config publish.config.ts",
    "build": "vite build",
    "commit": "cz",
    "dev:rerun": "npm run dev:clear && npm run dev",
    "dev:clear": "rm -rf node_modules/.vite && rm -rf dist",
    "dev": "netlify dev --functionsPort 7200",
    "preview": "npm run build -w && npm run serve dist"
  },
  "dependencies": {
    "@auth0/auth0-vue": "^2.0.0",
    "@img-comparison-slider/vue": "^7.4.3",
    "@netlify/functions": "^0.11.0",
    "@popperjs/core": "^2.11.2",
    "@serverless-jwt/jwt-verifier": "^0.2.1",
    "@turf/turf": "^6.5.0",
    "@vueuse/head": "^0.7.5",
    "ajv": "^8.10.0",
    "autoprefixer": "^10.4.4",
    "biketag": "^3.2.7",
    "bootstrap": "^5.1.3",
    "bootstrap-vue-next": "^0.15.5",
    "crypto-js": "^4.1.1",
    "dom-to-image": "^2.6.0",
    "exifr": "^7.1.3",
    "jose": "^4.6.0",
    "liquidjs": "^9.34.0",
    "loglevel": "^1.8.0",
    "lzutf8": "^0.6.1",
    "md5": "^2.3.0",
    "mitt": "^3.0.0",
    "netlify-identity-widget": "^1.9.2",
    "nodemailer": "^6.7.2",
    "pinia": "^2.1.7",
    "qs": "^6.10.3",
    "register-service-worker": "^1.7.2",
    "swiper": "^8.0.3",
    "v-lazy-image": "^2.1.1",
    "vue": "^3.3.8",
    "vue-confetti-explosion": "^1.0.2",
    "vue-google-maps-community-fork": "^0.3.1",
    "vue-i18n": "^9.2.2",
    "vue-iframes": "^0.0.19",
    "vue-loading-overlay": "^5.0.3",
    "vue-router": "^4.0.12",
    "vue-socials": "^2.0.4",
    "vue-timer-hook": "^0.0.22",
    "vue-toast-notification": "3.0",
    "vue3-cookies": "^1.0.6",
    "vue3-markdown-it": "^1.0.10"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/crypto-js": "^4.1.3",
    "@types/device-uuid": "^1.0.2",
    "@types/dom-to-image": "^2.6.6",
    "@types/markdown-it": "^13.0.5",
    "@types/md5": "^2.3.4",
    "@types/node": "^20.8.10",
    "@types/nodemailer": "^6.4.13",
    "@types/request": "^2.48.11",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "netlify-cli": "^16.9.3",
    "path": "^0.12.7",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-selector-bem-pattern": "^3.0.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.4.2",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-pwa": "^0.16.6",
    "workbox-build": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-window": "^7.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.ts": [
      "eslint . --fix"
    ],
    "*.{js,css,md,yml,yaml,json}": "prettier . --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "keywords": [
    "typescript",
    "biketag",
    "vue3",
    "vue",
    "vuejs"
  ],
  "types": "./index.d.ts",
  "module": "./biketag-vue.es.js",
  "exports": {
    ".": {
      "import": "./biketag-vue.es.js"
    },
    "./biketag-vue.css": {
      "import": "./biketag-vue.css"
    }
  }
}
